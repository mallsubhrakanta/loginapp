pipeline {
   agent any
   tools {
     maven "maven"  
   }

   stages {
      stage('Git Checkout') {
         steps {
            git 'https://github.com/srikanta1219/loginapp.git'   
         }
      }

    stage ('Maven Goal'){
      steps {
         sh "mvn clean install package    " 
      }    
    }
    stage ('Deploying Artifact'){
      steps {
        sh "mvn deploy "
       }
     
     
    }


    }
}
